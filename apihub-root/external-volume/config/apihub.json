{
  "storage": "../apihub-root",
  "port": 8080,
  "preventRateLimit": true,
  "activeComponents": [
    "metrics-middleware",
    "admin",
    "cloudWallet",
    "virtualMQ",
    "messaging",
    "notifications",
    "filesManager",
    "bdns",
    "bricksLedger",
    "bricksFabric",
    "bricking",
    "anchoring",
    "debugLogger",
    "mq",
    "secrets",
    "staticServer"
  ],
  "componentsConfig": {
    "staticServer": {
      "excludedFiles": [
        ".*.secret"
      ]
    },
    "bricking": {},
    "anchoring": {},
    "metrics-middleware": {
	"module": "./../../metrics-middleware/middleware"
    }
  },
  "responseHeaders": {
    "X-Frame-Options": "SAMEORIGIN",
    "X-XSS-Protection": "1; mode=block"
  },
  "enableRequestLogger": true,
  "enableJWTAuthorisation": false,
  "enableOAuth": false,
  "enableLocalhostAuthorization": false,
  "serverAuthentication": false,
  "skipOAuth": [
    "/assets",
    "/bdns",
    "/bundles",
    "/getAuthorization",
    "/external-volume/config/oauthConfig.js"
  ],
  "CORS": {
    "Access-Control-Allow-Origin": "*"
  }
}
